{
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/client/shared/assets/src",
  "prefix": "involvemint",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/client/shared/assets/src/**/*.ts",
          "libs/client/shared/assets/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/libs/client/shared/assets"],
      "options": {
        "jestConfig": "libs/client/shared/assets/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
